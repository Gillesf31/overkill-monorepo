{
  "name": "user-management-feature-shell",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/user-management/feature-shell/src",
  "prefix": "overkill-monorepo",
  "tags": [
    "scope:user-management",
    "type:routes"
  ],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/user-management/feature-shell/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/user-management/feature-shell/**/*.ts",
          "libs/user-management/feature-shell/**/*.html"
        ]
      }
    }
  }
}
